{
  "name": "bottomlessbrunch.com",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "classnames": "^2.2.5",
    "express": "^4.16.2",
    "google-map-react": "^0.26.0",
    "google-maps-api": "^2.0.1",
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "path": "^0.12.7",
    "prop-types": "^15.5.10",
    "rc-slider": "^8.5.0",
    "react": "^16.2.0",
    "react-animate-height": "^0.10.9",
    "react-dom": "^16.2.0",
    "react-ga": "^2.2.0",
    "react-google-maps": "^7.3.0",
    "react-helmet": "^5.2.0",
    "react-places-autocomplete": "^5.4.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-scripts": "1.0.13",
    "react-select": "^1.0.0-rc.10",
    "react-share": "^1.16.0",
    "react-slick": "^0.16.0",
    "react-stars": "^2.2.5",
    "react-swipeable": "^4.1.0",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "redux-thunk": "^2.2.0",
    "stringify-object": "^3.2.1",
    "viewport-units-buggyfill": "^0.6.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "coverage": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "yarn run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "dev": "concurrently 'npm run client' 'npm run server' 'npm run watch-css'",
    "deploy": "yarn run build && git add . && git commit -m 'Buildout' && git push && pm2 deploy ecosystem.config.js production"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "node-sass-chokidar": "^0.0.3",
    "redux-devtools-extension": "^2.13.2"
  },
  "proxy": {
    "/api/v1": {
      "target": "http://localhost:3021/"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/tempPolyfills.js",
      "!src/setupTests.js"
    ]
  },
  "standard": {
    "globals": [
      "jest",
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ]
  }
}
